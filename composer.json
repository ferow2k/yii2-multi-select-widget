	],
	"type": "yii2-extension",
	"homepage": "https://github.com/cusodede/yii2-multi-select-widget",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "2amigOS! Consulting Group",
			"email": "hola@2amigos.us",
			"homepage": "http://2amigos.us",
			"role": "Developer"
		},
		{
			"name": "Pavel Dubrovsky",
